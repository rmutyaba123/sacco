	<queryString>
		<![CDATA[SELECT markid, grade, existingid, firstquarter, studentname,
getcummgpa(getstudentdegreeid(existingid, addacademicyear(firstquarter, 0)),
 addacademicyear(firstquarter, 0)) as cummgpa0,
getcummgpa(getstudentdegreeid(existingid, addacademicyear(firstquarter, 1)),
 addacademicyear(firstquarter, 1)) as cummgpa1,
getcummgpa(getstudentdegreeid(existingid, addacademicyear(firstquarter, 2)),
 addacademicyear(firstquarter, 2)) as cummgpa2,
getcummgpa(getstudentdegreeid(existingid, addacademicyear(firstquarter, 3)),
 addacademicyear(firstquarter, 3)) as cummgpa3
FROM studentmarkview
WHERE (substring(firstquarter from 1 for 9) = '$P!{filterid}')
ORDER BY studentname;]]>
