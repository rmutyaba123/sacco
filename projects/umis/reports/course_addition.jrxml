	<queryString>
		<![CDATA[SELECT qcourseview.levellocationname, qcourseview.departmentname,
qcourseview.courseid, qcourseview.coursetitle, qcourseview.instructorname,
qstudentview.studentname, qgrades.gradeid, qcourseview.quarterid, qgrades.selectiondate
FROM qcourseview INNER JOIN qgrades ON qcourseview.qcourseid = qgrades.qcourseid
INNER JOIN qstudentview ON qstudentview.qstudentid = qgrades.qstudentid
WHERE (qgrades.selectiondate >= '$P!{startrange}') AND (qgrades.selectiondate <= '$P!{endrange}')
ORDER BY qgrades.selectiondate desc]]>
	</queryString>
